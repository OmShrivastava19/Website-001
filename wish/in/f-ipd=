<script>

	var country_code = 'IN';

	var current_url = window.location.href;
	var current_url_array = current_url.split('/');
	var current_url_array_len = current_url_array.length;

	var festival_arr = current_url_array[current_url_array_len-1].split('?');

	// for new website
	var f_hyphen = '/';
	if (festival_arr[0].indexOf('f-') > -1) {
		f_hyphen = '/f-';
	}
	festival_arr[0] = festival_arr[0].replace('f-', '');

	if ( festival_arr[0] == 'fh'  || festival_arr[0] == 'ipd'   || festival_arr[0] == 'isn'  ) {
		// do nothing
	}


	else if(festival_arr.length > 1){
		if (current_url_array_len == '4') { // definitely english locale ( could be other locale also ex abraisto)
			// window.location.href = current_url_array[0] + '//' + current_url_array[2] + f_hyphen + "e3?" + festival_arr[1];

			// Redirect eid fitr to eid adha
			if (['ef', 'ef1', 'ef2', 'ef3', 'rd', 'me', 'ea', 'ea1', 'ea2', 'isn', 'isn1'].indexOf(festival_arr[0]) > -1 ) {
				window.location.href = current_url_array[0] + '//' + current_url_array[2] + f_hyphen + "isn?" + festival_arr[1];
			}

			// halloween to thanksgiving
			if (festival_arr[0] == 'h' || festival_arr[0] == 'td') {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + f_hyphen + "ch?" + festival_arr[1];
			}

			
		}

		else{

			// if (festival_arr[0] == 'h') {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ch?" + festival_arr[1];
			// }


			// Hebrew people start

			// All israely
			// if (festival_arr[0] == 'pu' || festival_arr[0] == 'pu1' || festival_arr[0] == 'pe' || festival_arr[0] == 'je') {
				if ( ['il'].indexOf(current_url_array[3]) > -1 ) {
					// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "pen?" + festival_arr[1];
				}
			// }


			// All israely language is not hebrew
			if ( festival_arr[0] == 'yk' || festival_arr[0] == 'ros' || festival_arr[0] == 'pe' || festival_arr[0] == 'je' || festival_arr[0] == 'suk' || festival_arr[0] == 'sm' || festival_arr[0] == 'hn') {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "pen?" + festival_arr[1];
			}
			// Hebrew people end



			if ( ['ar'].indexOf(current_url_array[3]) > -1 ) {
				window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "isn?" + festival_arr[1];
			}

			else if ( ['tr'].indexOf(current_url_array[3]) > -1 ) {
				window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "isn?" + festival_arr[1];
			}

			else if ( ['id'].indexOf(current_url_array[3]) > -1 ) {
				if (festival_arr[0] != 'e' || festival_arr[0] != 'e1'){
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "isn?" + festival_arr[1];
				}
			}
			else if ( ['ms'].indexOf(current_url_array[3]) > -1 ) {
				window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "isn?" + festival_arr[1];
			}



			else if (['ef', 'ef1', 'ef2', 'ef3', 'rd', 'me', 'ea', 'ea1', 'ea2', 'isn', 'isn1'].indexOf(festival_arr[0]) > -1) {
				window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "isn?" + festival_arr[1];
			}


			// else if (festival_arr[0] == 'fd' || festival_arr[0] == 'fd1' || festival_arr[0] == 'fd2' ) {
			// 	window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + 'in' + "/gc?" + festival_arr[1];
			// }


			

			else if ( ['ru'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "rsd?" + festival_arr[1];
			}

			else if ( ['en'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "m?" + festival_arr[1];
			}


			// do not redirect islamic festivals
			else if ( ['ef', 'ef1', 'ef2', 'ef3', 'rd', 'me', 'ea', 'ea1', 'ea2', 'isn', 'isn1'].indexOf(festival_arr[0]) == -1 ){

				if ( ['in'].indexOf(current_url_array[3]) > -1 ) {
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ipd?" + festival_arr[1];
				}

				else if ( ['hi'].indexOf(current_url_array[3]) > -1 ) {
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ipd?" + festival_arr[1];
				}

				else if ( ['mr'].indexOf(current_url_array[3]) > -1 ) {
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ipd?" + festival_arr[1];
				}

				else if ( ['te'].indexOf(current_url_array[3]) > -1 ) {
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ipd?" + festival_arr[1];
				}

				else if ( ['ta'].indexOf(current_url_array[3]) > -1 ) {
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ipd?" + festival_arr[1];
				}

				else if ( ['bn'].indexOf(current_url_array[3]) > -1 ) {
					window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "ipd?" + festival_arr[1];
				}

			}


			




			else if ( ['uk'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "dd?" + festival_arr[1];
			}

			else if ( ['fi'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "m?" + festival_arr[1];
			}




			// else if ( ['de', 'it', 'fr', 'fi'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "m?" + festival_arr[1];
			// }


			else if ( ['de'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "cc?" + festival_arr[1];
			}


			else if ( ['it'].indexOf(current_url_array[3]) > -1 ) {
				window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "fh?" + festival_arr[1];
			}


			else if ( ['pt'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "fd?" + festival_arr[1];
			}

			else if ( ['es'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "fd?" + festival_arr[1];
			}


			else if ( ['it', 'es', 'pt', 'de', 'nl', 'ru', 'pl', 'hr', 'fr', 'ro', 'uk'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "e?" + festival_arr[1];
			}

			// else if ( ['it', 'es', 'de', 'nl', 'ru', 'fr'].indexOf(current_url_array[3]) > -1 ) {
				// window.location.href = current_url_array[0] + '//' + current_url_array[2] + '/' + current_url_array[3] + f_hyphen + "v?" + festival_arr[1];
