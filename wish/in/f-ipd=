    <script>

        var current_locale = 'in'; 
        var current_url_arr = window.location.href.split('/');
        var query_string = current_url_arr[current_url_arr.length-1];
        var domain = window.location.protocol + "//" + window.location.hostname;

        var urls = ['ta', 'te', 'bn', 'mr', 'hi'];
        for (var i = 0; i <= urls.length; i++) {
            var updated_url = domain + "/" + urls[i] + "/" + query_string + "&utm_source="+current_locale+"&utm_medium="+urls[i]+"&utm_campaign=na";
            $('#'+urls[i]+'_custom_lang_group').attr("href",  updated_url);
        }
